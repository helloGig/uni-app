<view><tabs vue-id="1" TabList="{{TabList}}" currentTab="{{current}}" data-event-opts="{{[['^tabs',[['tabsChange']]]]}}" bind:tabs="__e" bind:__l="__l" vue-slots="{{['default']}}"><tab-pane vue-id="{{('2')+','+('1')}}" bind:__l="__l" vue-slots="{{['default']}}"><view><scroll-view class="container" scroll-y="true"><block wx:for="{{orderList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="orderList"><view class="order-title"><view class="order-title-restaurantName">{{''+item.restaurantName}}</view><view class="order-title-state">{{item.state}}</view></view><view class="order-content"><image class="order-content-restaurantImg" src="{{item.src}}"></image><view class="order-content-info"><view class="order-content-info-price">{{"￥ "+item.price}}</view><view class="order-content-info-date">{{''+item.time}}</view><view class="order-content-info-howToDistribute">{{'由 '+item.howToDistribute+" 提供服务"}}</view></view></view><view class="order-footer"><view data-event-opts="{{[['tap',[['anotherList']]]]}}" class="order-footer-orderAgain" catchtap="__e">再来一单</view><view data-event-opts="{{[['tap',[['comment']]]]}}" class="order-footer-goToComment" catchtap="__e">去评论</view></view></view></block></block></scroll-view></view></tab-pane><tab-pane vue-id="{{('3')+','+('1')}}" bind:__l="__l" vue-slots="{{['default']}}"><view class="commentPage-header"><view class="commentPage-score"><view class="overall">综合评分</view>4.3<view class="rate">商家综合好评率为76%</view></view><view class="commentPage-rating"><view class="restaurant">商家评分<view class="stars" style="color:#C6E746;">★★★★★</view></view><view class="distribute">配送评分<view class="stars" style="color:#C6E746;">★★★★★</view></view></view></view><view class="commentPage-category"><block wx:for="{{categoryList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="{{['category-item',active_flag==index?'category-item-active':'']}}" data-id="{{index}}" data-event-opts="{{[['tap',[['category_active',['$event']]]]]}}" catchtap="__e">{{item.name+"("+item.Nub+")"}}</view></block></block></view><view class="uni-padding-wrap"><view class="uni-comment"><block wx:for="{{critic}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="uni-comment-list"><view class="uni-comment-face"><image src="{{item.handPic}}" mode="widthFix"></image></view><view class="uni-comment-body"><view class="uni-comment-top"><text>{{item.name}}</text></view><view class="uni-comment-content">{{item.content}}</view></view></view></block></block></view></view></tab-pane><tab-pane vue-id="{{('4')+','+('1')}}" bind:__l="__l" vue-slots="{{['default']}}"><view><scroll-view class="container" scroll-y="true"><block wx:for="{{orderList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="orderList"><view class="order-title"><view class="order-title-restaurantName">{{''+item.restaurantName}}</view><view class="order-title-state">{{item.state}}</view></view><view class="order-content"><image class="order-content-restaurantImg" src="{{item.src}}"></image><view class="order-content-info"><view class="order-content-info-price">{{"￥ "+item.price}}</view><view class="order-content-info-date">{{''+item.time}}</view><view class="order-content-info-howToDistribute">{{'由 '+item.howToDistribute+" 提供服务"}}</view></view></view><view class="order-footer"><view data-event-opts="{{[['tap',[['anotherList']]]]}}" class="order-footer-orderAgain" catchtap="__e">再来一单</view><view data-event-opts="{{[['tap',[['refund']]]]}}" class="order-footer-goToComment" catchtap="__e">申请退款</view></view></view></block></block></scroll-view></view></tab-pane></tabs></view>